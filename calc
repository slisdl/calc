class Calc:

  @staticmethod
  def add(a, b):
    return a + b

  @staticmethod
  def sub(a, b):
      return a - b

  @staticmethod
  def mult(a, b):
    return a * b

  @staticmethod
  def div(a, b):
    return a / b

  @staticmethod
  def exp(a, b):
    return a ** b

  @staticmethod
  def int_div(a, b):
    if b == 0:
      raise ValueError("Cannot divide by zero")
    return a // b

  @staticmethod
  def mod(a, b):
    if b == 0:
      raise ValueError("Cannot divide by zero")
    return a % b







